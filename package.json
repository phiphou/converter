    "url": "https://github.com/phiphou/converter/issues"
  },
  "description": "Un convertisseur pour journaliste",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint . --max-warnings=0",
      "prettier --check"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --cache --fix --ignore-pattern .gitignore --ext .ts,.tsx .",
    "preview": "vite preview",
    "format": "prettier --log-level error -uw --cache --ignore-path /.prettierignore .",
    "validate": "run-s typecheck format lint",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@types/node": "^22.15.17",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.9",
    "color-convert": "^3.0.1",
    "fb-tiger-hash": "^1.0.0",
    "hash-wasm": "^4.12.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tailwindcss/vite": "4.1.6",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.6",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "6.3.5",
    "vite-bundle-analyzer": "^0.20.2",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
